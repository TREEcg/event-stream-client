{
  "name": "@brechtvdv/event-stream-client",
  "version": "0.0.5",
  "description": "Syncs an entire TREE collection, but only emits the objects, does not store them. ",
  "main": "dist/sync.js",
  "bin": "bin/cli.js",
  "scripts": {
    "build": "node \"./node_modules/typescript/bin/tsc\"",
    "test": "node ./dist/example.js"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "test/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TREEcg/event-stream-client.git"
  },
  "keywords": [
    "event",
    "stream",
    "TREE",
    "hypermedia",
    "Web",
    "API"
  ],
  "author": "Brecht Van de Vyvere",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TREEcg/event-stream-client/issues"
  },
  "homepage": "https://github.com/TREEcg/event-stream-client#readme",
  "dependencies": {
    "@rdfjs/data-model": "^1.2.0",
    "awesome-typescript-loader": "^5.2.1",
    "cacheable-request": "^7.0.1",
    "commander": "^6.2.0",
    "follow-redirects": "^1.13.0",
    "http-cache-semantics": "^4.1.0",
    "js-priority-queue": "^0.1.5",
    "jsonld-streaming-serializer": "^1.2.0",
    "n3": "^1.6.4",
    "rdf-js": "^1.0.2",
    "rdf-object": "^1.7.1",
    "rdf-parse": "^1.6.0",
    "source-map-loader": "^2.0.0",
    "streamify-string": "^1.0.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/n3": "^1.0.4",
    "@types/rdf-js": "^2.0.1",
    "jest": "^26.6.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1"
  }
}
