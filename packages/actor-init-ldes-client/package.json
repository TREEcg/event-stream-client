{
  "name": "@treecg/actor-init-ldes-client",
  "version": "2.3.5",
  "description": "An init actor that fetches members from an Event Stream API",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/treecg/event-stream-client.git",
    "directory": "packages/actor-init-ldes-client"
  },
  "lsd:module": true,
  "main": "index.js",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "bin/**/*.d.ts",
    "bin/**/*.js",
    "config",
    "components",
    "engine-default.js",
    "index.d.ts",
    "index.js"
  ],
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "../../tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))-test.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "dependencies": {
    "@comunica/actor-http-node-fetch": "1.21.2",
    "@comunica/actor-http-proxy": "1.21.1",
    "@comunica/actor-init-sparql": "1.21.3",
    "@comunica/actor-rdf-dereference-http-parse": "1.21.2",
    "@comunica/actor-rdf-parse-jsonld": "1.21.2",
    "@comunica/actor-rdf-parse-n3": "1.21.1",
    "@comunica/bus-http": "1.21.1",
    "@comunica/bus-init": "1.21.1",
    "@comunica/bus-rdf-dereference": "1.21.1",
    "@comunica/bus-rdf-metadata-extract": "1.21.1",
    "@comunica/bus-rdf-parse": "1.21.1",
    "@comunica/core": "1.21.1",
    "@comunica/mediator-combine-union": "1.21.1",
    "@comunica/mediator-number": "1.21.1",
    "@comunica/mediator-race": "1.21.1",
    "@comunica/runner": "1.21.1",
    "@comunica/runner-cli": "1.21.1",
    "@dexagod/rdf-retrieval": "^1.0.2",
    "@rdfjs/data-model": "1.3.3",
    "@treecg/actor-rdf-filter-object-with-framing": "^2.3.4",
    "@treecg/actor-rdf-filter-objects-with-quadstore": "^2.3.4",
    "@treecg/actor-rdf-frame-with-json-ld-js": "^2.3.4",
    "@treecg/actor-rdf-metadata-extract-tree": "^1.20.0",
    "@treecg/bus-rdf-filter-object": "^2.3.4",
    "@treecg/bus-rdf-frame": "^2.3.4",
    "awesome-typescript-loader": "^5.2.1",
    "cacheable-request": "^7.0.1",
    "follow-redirects": "^1.13.0",
    "http-cache-semantics": "^4.1.0",
    "js-priority-queue": "^0.1.5",
    "lru-cache": "^6.0.0",
    "moment": "^2.29.1",
    "n3": "^1.8.0",
    "rdf-dereference": "^1.8.0",
    "source-map-loader": "^2.0.0",
    "streamify-string": "^1.0.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/n3": "^1.0.4",
    "@types/rdf-js": "^4.0.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.2",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1"
  },
  "bin": {
    "actor-init-ldes-client": "./bin/run.js"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "node \"../../node_modules/jest/bin/jest.js\" ${1}",
    "test-watch": "node \"../../node_modules/jest/bin/jest.js\" ${1} --watch",
    "prepare": "comunica-compile-config config/config-default.json urn:comunica:myLDESClient > engine-default.js",
    "build": "node \"../../node_modules/typescript/bin/tsc\"",
    "start": "yarn build | node ./bin/run.js"
  },
  "author": "",
  "license": "ISC",
  "gitHead": "7e3418ef6a15a38e973f5651d6e67a83bd5cf421"
}
